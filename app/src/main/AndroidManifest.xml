    <queries>
        <package android:name="com.whatsapp" />
    </queries>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:hardwareAccelerated="true"
        android:theme="@style/Theme.WhatsTool"
        android:extractNativeLibs="true"
        tools:replace="android:extractNativeLibs"
        tools:targetApi="31">
        <activity
            android:name=".instareshare.InstaReshareActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".textmagic.TextMagicActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name="com.moutamid.whatstool.statussaver.StatusSaverMainActivity"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.moutamid.whatstool.statussaver.RecentStoriesActivity"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.moutamid.whatstool.statussaver.SavedStoriesActivity"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.moutamid.whatstool.statussaver.VideoPlayers"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.moutamid.whatstool.statussaver.ImageViewer"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.moutamid.whatstool.statussaver.SavedVideoPlayer"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.moutamid.whatstool.statussaver.VideoPlayer"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.moutamid.whatstool.statussaver.SavedImageViewer"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".emotions.EmotionsActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".TextToEmojiActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".FakeProfileActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".MakeProfileActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".WhatsAppShortcutActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".CaptionsItemActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".CaptionsActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".StartScannerActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".TextRepeaterActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".QRScannerActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".QRGeneratorActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".StoryFullScreenActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".MakeStoryActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
            android:name=".DirectChatActivity"
            android:hardwareAccelerated="true"
            android:exported="false" />
        <activity
